    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "059999e4-e93f-42f0-b36c-f6e465381162",
        "outputId": "22fc1529-927f-4123-cb09-4e484c26ec9a"
      },
      "outputs": [
        {
          "name": "stdout",
          "output_type": "stream",
